{
  "name": "@yourusername/training-plan-generator",
  "version": "1.0.0",
  "description": "Advanced scientific training plan generation for running",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
