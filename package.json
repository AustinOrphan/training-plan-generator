    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "date-fns": "^3.0.0"
  },
  "peerDependencies": {
    "date-fns": ">=2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/training-plan-generator.git"
  },
